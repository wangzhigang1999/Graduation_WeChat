<view>
<text class="SubSubTitle">添加贴纸合成您的照片</text>
<canvas-drag class="canvas" id="canvas-drag" graph="{{graph}}" width="460" height="960" enableUndo="{{true}}"></canvas-drag>


<view class='bottom'>

  <view class="sticker-lists-body">
    <scroll-view class="recommend_scroll_x_box" scroll-x="true">
      <view class="sticker-list" wx:for="{{stickers}}" data-url="{{item}}" bindtap='addImg'>
        <image src='{{item}}'></image>
      </view>
    </scroll-view>
  </view>
</view>
 <button class="ConfirmBtn" bindtap="BtnToConfirm">完成</button>
</view>



